    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>TraineeCourse</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Trainee.TraineeUserID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
